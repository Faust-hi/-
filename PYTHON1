my_list = [1, 2, 3, 4, 5]
fruits = ["apple", "banana", "orange"]
mixed = [1, "hello", 3.14, True]

my_list.append(6)
my_list.remove(3)

print(my_list[0])
print(len(my_list))
print(my_list[1:4])

for item in my_list:
    print(item)
for i, fruits in enumerate(fruits):
    print(f'Index {i}:{fruits}')
